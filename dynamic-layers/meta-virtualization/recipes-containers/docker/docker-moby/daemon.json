{
    "experimental": true,
    "features": { "buildkit": false, "containerd-snapshotter": true },
    "exec-opts": [],
    "exec-root": "/var/run/docker",
    "data-root": "/var/lib/docker",
    "log-driver": "journald",
    "log-level": "info",
    "insecure-registries": [],
